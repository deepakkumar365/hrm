<li><a class=\"dropdown-item\" href=\"{{ url_for('attendance_list') }}\">\n                                <i class=\"fas fa-list me-1\"></i>View Records\n                            </a></li>\n                        </ul>\n                    </li>\n                    \n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link dropdown-toggle\" href=\"#\" data-bs-toggle=\"dropdown\">\n                            <i class=\"fas fa-plane-departure me-1\"></i>Leave\n                        </a>\n                        <ul class=\"dropdown-menu\">\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('leave_request') }}\">\n                                <i class=\"fas fa-plus me-1\"></i>Request Leave\n                            </a></li>\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('leave_list') }}\">\n                                <i class=\"fas fa-list me-1\"></i>View Requests\n                            </a></li>\n                        </ul>\n                    </li>\n                    \n                    {% if current_user.role in ['Super Admin', 'Admin', 'Manager'] %}\n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link dropdown-toggle\" href=\"#\" data-bs-toggle=\"dropdown\">\n                            <i class=\"fas fa-dollar-sign me-1\"></i>Payroll\n                        </a>\n                        <ul class=\"dropdown-menu\">\n                            <li>\n                                <a class=\"dropdown-item\" href=\"{{ url_for('payroll_list') }}\">\n                                    <i class=\"fas fa-list me-1\"></i>Payroll List\n                                </a>\n                            </li>\n                            <li>\n                                <a class=\"dropdown-item\" href=\"{{ url_for('payroll_generate') }}\">\n                                    <i class=\"fas fa-calculator me-1\"></i>Generate Payroll\n                                </a>\n                            </li>\n                            <li>\n                                <a class=\"dropdown-item\" href=\"{{ url_for('payroll_config') }}\">\n                                    <i class=\"fas fa-sliders-h me-1\"></i>Payroll Configuration\n                                </a>\n                            </li>\n                        </ul>\n                    </li>\n                    {% endif %}\n                    \n                    {% if current_user.role in ['Super Admin', 'Admin'] %}\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" href=\"{{ url_for('compliance_dashboard') }}\">\n                            <i class=\"fas fa-file-alt me-1\"></i>Compliance\n                        </a>\n                    </li>\n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link dropdown-toggle\" href=\"#\" data-bs-toggle=\"dropdown\">\n                            <i class=\"fas fa-cogs me-1\"></i>Masters\n                        </a>\n                        <ul class=\"dropdown-menu\">\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('role_list') }}\">\n                                <i class=\"fas fa-user-tag me-1\"></i>Roles\n                            </a></li>\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('department_list') }}\">\n                                <i class=\"fas fa-building me-1\"></i>Departments\n                            </a></li>\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('working_hours_list') }}\">\n                                <i class=\"fas fa-clock me-1\"></i>Working Hours\n                            </a></li>\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('work_schedule_list') }}\">\n                                <i class=\"fas fa-calendar-alt me-1\"></i>Work Schedules\n                            </a></li>\n                        </ul>\n                    </li>\n<!--                    <li class=\"nav-item\">-->\n<!--                        <a class=\"nav-link\" href=\"{{ url_for('user_management') }}\">-->\n<!--                            <i class=\"fas fa-user-cog me-1\"></i>Users-->\n<!--                        </a>-->\n<!--                    </li>-->\n                    {% endif %}\n                </ul>\n                \n                <ul class=\"navbar-nav\">\n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link dropdown-toggle\" href=\"#\" data-bs-toggle=\"dropdown\">\n                            <img src=\"{{ current_user.profile_image_url or 'data:image/svg+xml,<svg xmlns=\\\"http://www.w3.org/2000/svg\\\" viewBox=\\\"0 0 100 100\\\"><circle cx=\\\"50\\\" cy=\\\"50\\\" r=\\\"40\\\" fill=\\\"%23007bff\\\"/><text x=\\\"50\\\" y=\\\"60\\\" text-anchor=\\\"middle\\\" fill=\\\"white\\\" font-size=\\\"40\\\">üë§</text></svg>' }}\" \n                                 alt=\"Profile\" class=\"rounded-circle me-1\" width=\"24\" height=\"24\" style=\"object-fit: cover;\">\n                            {{ current_user.first_name or 'User' }}\n                        </a>\n                        <ul class=\"dropdown-menu dropdown-menu-end\">\n                            <li><span class=\"dropdown-item-text\">\n                                <small class=\"text-muted\">{{ current_user.role }}</small>\n                            </span></li>\n                            <li><hr class=\"dropdown-divider\"></li>\n                            {% if hasattr(current_user, 'employee_profile') and current_user.employee_profile %}\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('profile') }}\">\n                                <i class=\"fas fa-user me-1\"></i>My Profile\n                            </a></li>\n                            {% endif %}\n                            <li><a class=\"dropdown-item\" href=\"{{ url_for('logout') }}\">\n                                <i class=\"fas fa-sign-out-alt me-1\"></i>Logout\n                            </a></li>\n                        </ul>\n                    </li>\n                </ul>\n            </div>\n            {% endif %}\n        </div>\n    </nav>\n\n    <!-- Main content with mobile spacing -->\n    <main class=\"container-fluid\" style=\"margin-top: 76px; padding-bottom: 20px;\">\n        {% with messages = get_flashed_messages(with_categories=true) %}\n            {% if messages %}\n                {% for category, message in messages %}\n                <div class=\"alert alert-{{ 'danger' if category == 'error' else category }} alert-dismissible fade show mt-3\" role=\"alert\">\n                    {{ message }}\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\"></button>\n                </div>\n                {% endfor %}\n            {% endif %}\n        {% endwith %}\n\n        {% block content %}{% endblock %}\n    </main>\n\n    <!-- Mobile quick actions floating button -->\n    {% if current_user.is_authenticated and current_user.role == 'User' %}\n    <div class=\"position-fixed bottom-0 end-0 p-3\" style=\"z-index: 1050;\">\n        <div class=\"dropdown dropup\">\n            <button class=\"btn btn-primary rounded-circle shadow\" style=\"width: 56px; height: 56px;\" \n                    data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n                <i class=\"fas fa-plus\"></i>\n            </button>\n            <ul class=\"dropdown-menu dropdown-menu-end mb-2\">\n                <li><a class=\"dropdown-item\" href=\"{{ url_for('attendance_mark') }}\">\n                    <i class=\"fas fa-clock me-2\"></i>Mark Attendance\n                </a></li>\n                <li><a class=\"dropdown-item\" href=\"{{ url_for('leave_request') }}\">\n                    <i class=\"fas fa-plane-departure me-2\"></i>Request Leave\n                </a></li>\n                <li><a class=\"dropdown-item\" href=\"{{ url_for('claims_submit') }}\">\n                    <i class=\"fas fa-receipt me-2\"></i>Submit Claim\n                </a></li>\n            </ul>\n        </div>\n    </div>\n    {% endif %}\n\n    <!-- Bootstrap JS -->\n    <script src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"></script>\n    \n    <!-- Mobile dropdown fix -->\n    <script>\n    document.addEventListener('DOMContentLoaded', function() {\n        // Mobile dropdown handler\n        function handleMobileDropdowns() {\n            const dropdownToggles = document.querySelectorAll('.navbar-nav .dropdown-toggle');\n            \n            dropdownToggles.forEach(toggle => {\n                // Remove existing listeners\n                toggle.removeEventListener('click', handleDropdownClick);\n                \n                // Add new listener for mobile\n                if (window.innerWidth <= 991) {\n                    toggle.addEventListener('click', handleDropdownClick);\n                }\n            });\n        }\n        \n        function handleDropdownClick(e) {\n            e.preventDefault();\n            e.stopPropagation();\n            \n            const dropdown = this.closest('.dropdown');\n            const menu = dropdown.querySelector('.dropdown-menu');\n            const isOpen = dropdown.classList.contains('show');\n            \n            // Close all dropdowns first\n            document.querySelectorAll('.navbar-nav .dropdown').forEach(d => {\n                d.classList.remove('show');\n            });\n            \n            // Toggle current dropdown if it wasn't open\n            if (!isOpen) {\n                dropdown.classList.add('show');\n            }\n        }\n        \n        // Initialize\n        handleMobileDropdowns();\n        \n        // Re-initialize on window resize\n        window.addEventListener('resize', handleMobileDropdowns);\n    });\n    </script>\n    \n    <!-- Custom JS -->\n    <script src=\"{{ url_for('static', filename='js/app.js') }}\"></script>\n    \n    {% block scripts %}{% endblock %}\n</body>\n</html>\n",
